{
	"useTabs": true,
	"tabWidth": 2,
	"printWidth": 100,
	"singleQuote": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"semi": true,
	"arrowParens": "avoid",
	"plugins": ["prettier-plugin-svelte"],
	"svelteStrictMode": false,
	"svelteBracketNewLine": true,
	"svelteIndentScriptAndStyle": true,
	"svelteSortOrder": "options-scripts-styles-markup"
}
